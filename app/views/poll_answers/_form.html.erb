<%= simple_form_for(@poll_answer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :text %>
    <%= f.input :poll_question_id %>
    <%= f.input :ordinal %>
    <%= f.input :free_response %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
