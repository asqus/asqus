<h1>Listing state_senate_districts</h1>

<table>
  <tr>
    <th>District number</th>
    <th>State</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @state_senate_districts.each do |state_senate_district| %>
  <tr>
    <td><%= state_senate_district.district_number %></td>
    <td><%= state_senate_district.state_id %></td>
    <td><%= link_to 'Show', state_senate_district %></td>
    <td><%= link_to 'Edit', edit_state_senate_district_path(state_senate_district) %></td>
    <td><%= link_to 'Destroy', state_senate_district, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

