<h1>Listing state_house_districts</h1>

<table>
  <tr>
    <th>District number</th>
    <th>State</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @state_house_districts.each do |state_house_district| %>
  <tr>
    <td><%= state_house_district.district_number %></td>
    <td><%= state_house_district.state_id %></td>
    <td><%= link_to 'Show', state_house_district %></td>
    <td><%= link_to 'Edit', edit_state_house_district_path(state_house_district) %></td>
    <td><%= link_to 'Destroy', state_house_district, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

